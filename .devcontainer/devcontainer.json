{
  "name": "Node.js",
  "build": {
    "dockerfile": "Dockerfile",
    "args": {
      "VARIANT": "14"
    }
  },
  "settings": {
    "terminal.integrated.shell.linux": "/bin/sh"
  },
  "extensions": [
    "dbaeumer.vscode-eslint",
    "svelte.svelte-vscode"
  ],
  "forwardPorts": [
    8080
  ],
  "postCreateCommand": "install",
  "remoteUser": "node"
}
